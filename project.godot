Util="*res://Scripts/Util.gd"
Notifier="*res://Scenes/Notifier.tscn"
BgmController="*res://Prefab/BGMController.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/AdmobPlugin/plugin.cfg", "res://addons/PGSGP/plugin.cfg")


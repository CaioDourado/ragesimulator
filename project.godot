run/max_fps=60
boot_splash/bg_color=Color(0.0627451, 0.133333, 0.215686, 1)
boot_splash/image="res://Assets/Other/gb.png"
boot_splash/fullsize=false
config/icon="res://Assets/Icon/icon.png"

[autoload]

GameManager="*res://Scripts/GameManager.gd"
Loading="*res://Prefab/UI/loading.tscn"
AdManager="*res://Scenes/admob.tscn"
